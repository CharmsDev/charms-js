    "snarkjs": "^0.7.5"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "buffer": "^6.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.2.1"
  }
